{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose --no-coverage",
    "test:quiet": "jest --no-coverage",
    "test:with-logs": "set TEST_VERBOSE=1&& jest --no-coverage",
    "test:coverage": "jest --coverage --coverageReporters=text --coverageReporters=html",
    "format": "prettier --check \"src/**/*.{ts,js,json}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,js,json}\""
  },
  "author": "",
  "license": "ISC",
  "description": "TypeScript backend project",
  "dependencies": {
    "body-parser": "^2.2.1",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "firebase-admin": "^13.5.0",
    "google-auth-library": "^10.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2",
    "openai": "^6.5.0",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.0",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^10.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.6.2",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "body-parser": "^2.2.1"
  }
}
